/* pages/chat-list/dynamicInfo.wxss */
.redDot{
  width: 1rem;
  height: 1rem;
  color: #fff;
  text-align: center;
  line-height: 1rem;
  border-radius: 50%;
  float: right;
  margin-right: 20rpx;
}
.cu-list+.cu-list{
  margin-top: 0;
}
.more{
  z-index: 99;
  position: fixed;
  top: 90%;left: 80%;
  font-size: 30rpx;
  line-height: 100rpx;
  font-weight: 1200;
  width: 70rpx;
  height: 70rpx;
  background: #8d3ee7;
  text-align: center;
  box-shadow: 0rpx 0rpx 20rpx rgb(241, 237, 237);
}

.more::after{
  content: '';
  display: inline-block;
  width: 4rpx;
  height: 38rpx;
  background: #fff;
  position: absolute;
  left: 50%;top: 50%;transform: translate(-50%,-50%);
  border-radius: 5rpx;
}
.more::before{
  content:'';
  display: inline-block;
  width: 38rpx;
  height: 4rpx;
  background: #fff;
  position: absolute;
  left: 50%;top: 50%;transform: translate(-50%,-50%);
  border-radius: 5rpx;
}